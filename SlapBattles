	Name = "Get pillow point (100 point per click",
	Callback = function()
      		for i = 1, 100 do
        local args = {
            [1] = "AddPillow"
        }
        game:GetService("ReplicatedStorage").Events.PillowEvent:FireServer(unpack(args))
  	end    
})

